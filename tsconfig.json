{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    // "module": "commonjs",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": false,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strictNullChecks":false,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [],
    "outDir": "./dist"
  },
  "files": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "python"
  ]
}